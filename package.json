  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest run --coverage --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run type-check && npm run lint && npm run test:run && npm run test:e2e",
    "db:init": "node scripts/init-database.js",
    "db:seed": "node scripts/init-database.js",
    "test:api": "node scripts/test-api.js",
    "analyze": "cross-env ANALYZE=true next build",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "clean": "rimraf .next dist build coverage test-results playwright-report",
    "clean:all": "npm run clean && rimraf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "dev:setup": "node scripts/dev-setup.js",
    "dev:full": "npm run dev:setup && npm run dev",
    "prepare": "husky",
    "precommit": "lint-staged",
    "prepush": "npm run test:ci"
